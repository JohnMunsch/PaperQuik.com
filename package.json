{
  "name": "paperquik",
  "version": "2.0.0",
  "description": "A site where users can design printable pages.",
  "main": "index.mjs",
  "scripts": {
    "dev": "node index.mjs & esbuild public/js/main.js --bundle --outfile=public/js/bundle.js --watch",
    "docker": "esbuild public/js/main.js --bundle --outfile=public/js/bundle.js && docker build -t registry.gitlab.com/johnmunsch/paperquik . && docker push registry.gitlab.com/johnmunsch/paperquik",
    "docker-login": "docker login registry.gitlab.com",
    "docker-run": "docker run -p7080:7080 -it --rm --name paperquik registry.gitlab.com/johnmunsch/paperquik:latest",
    "start": "node index.mjs",
    "test": "web-test-runner \"public/js/**/*.test.js\" --node-resolve",
    "test:watch": "web-test-runner \"public/js/**/*.test.js\" --node-resolve --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/JohnMunsch/paperquik.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/JohnMunsch/paperquik/issues"
  },
  "homepage": "https://gitlab.com/JohnMunsch/paperquik#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "lit": "^2.0.0-rc.2",
    "page": "^1.11.6"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@open-wc/semantic-dom-diff": "^0.19.5",
    "@open-wc/testing": "^3.0.1",
    "@web/test-runner": "^0.13.20",
    "esbuild": "^0.12.5"
  }
}
