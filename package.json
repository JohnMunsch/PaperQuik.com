{
  "name": "paperquik",
  "version": "2.0.0",
  "description": "A site where users can design printable pages.",
  "main": "index.mjs",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/JohnMunsch/paperquik.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/JohnMunsch/paperquik/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@open-wc/semantic-dom-diff": "^0.19.5",
    "@open-wc/testing": "^3.1.2",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/web-components": "^6.4.22",
    "@web/test-runner": "^0.13.27",
    "babel-loader": "^8.2.5",
    "esbuild": "^0.14.36"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.3",
    "lit": "^2.2.2",
    "page": "^1.11.6"
  },
  "homepage": "https://www.PaperQuik.com",
  "scripts": {
    "dev": "node index.mjs & esbuild public/js/main.js --bundle --outfile=public/js/bundle.js --sourcemap --watch",
    "build": "esbuild public/js/main.js --bundle --outfile=public/js/bundle.js --sourcemap",
    "docker-build": "npm run build && docker build --platform linux/amd64 -t paperquik_amd . && docker image tag paperquik_amd:latest ghcr.io/johnmunsch/paperquik_amd:latest && docker push ghcr.io/johnmunsch/paperquik_amd:latest",
    "docker-login": "echo $CR_PAT | docker login ghcr.io -u USERNAME --password-stdin",
    "start": "node index.mjs",
    "test": "web-test-runner \"public/js/**/*.test.js\" --node-resolve",
    "test:watch": "web-test-runner \"public/js/**/*.test.js\" --node-resolve --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  }
}
